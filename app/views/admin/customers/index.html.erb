<div class="container">
  <h2>会員一覧</h2>
  </div>
   <div class="container">
    <table>
      <thead>
        <tr>
          <th>会員ID</th>
          <th>氏名</th>
          <th>メールアドレス</th>
          <th>ステータス</th>
        </tr>
      </thead>
      <tbody>
        <% @customers.each do |customer| %>
        <tr>
          <td><%= customer.id %></td>
          <td><%= customer.email %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
<%= paginate customers %>