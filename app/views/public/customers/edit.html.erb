<h1>会員情報編集</h1>
<%= form_with model: @customer do |f| %>
<div class="container">
<table>
  <tbody>
    <tr>
      <td>氏名</td>
      <td><%= f.text_field :last_name, autofocus: true, id:"inputLastName", placeholder:"姓" %></td>
      <td><%= f.text_field :first_name, autofocus: true, id:"inputFirstName", placeholder:"名" %></td>
    </tr>
    <tr>
      <td>フリガナ</td>
      <td><%= f.text_field :last_name_kana, autofocus: true, id:"inputLastNameKana", placeholder:"セイ" %></td>
      <td><%= f.text_field :first_name_kana, autofocus: true, id:"inputFirstNameKana", placeholder:"メイ" %></td>
    </tr>
    <tr>
      <td>郵便番号</td>
      <td><%= f.number_field :postal_code, autofocus: true, id:"inputPostalCode", placeholder:"郵便番号" %></td>
    </tr>
    <tr>
      <td>住所</td>
      <td><%= f.text_area :address, autofocus: true, id:"inputAddress", placeholder:"住所" %></td>
    </tr>
    <tr>
      <td>電話番号</td>
      <td><%= f.number_field :phone_number, autofocus: true, id:"inputTelePhoneNumber", placeholder:"電話番号" %></td>
    </tr>
    <tr>
      <td>メールアドレス</td>
      <td><%= f.text_field :email, autofocus: true, id:"inputEmail", placeholder:"メールアドレス" %></td>
    </tr>
  </tbody>
</table>


<%= f.submit "編集を保存する", class:"btn btn-success" %>
<% end %>
</div>

<div class="container">
  <%= link_to "退会する", quit_path, class:"btn btn-danger" %>
</div>